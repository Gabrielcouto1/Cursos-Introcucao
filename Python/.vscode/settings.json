{
    "files.exclude": {
        "**/*.exe": true,
        "**/*.code-workspace": true,
        "**/*.out": true,
        "**/.vscode":true,
        "**/*.pdf":true,
        "**/*.pyc":true,
        "**/__pycache__":true
    },
    "C_Cpp.errorSquiggles": "Disabled",
    "editor.fontFamily": "Fira Code",
    "editor.fontSize": 13,
    "editor.fontLigatures": true,
    "files.associations": {
        "math.h": "c",
        "rand-impar.h": "c"
    },
    "code-runner.runInTerminal": true
}